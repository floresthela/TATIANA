program factorial_recursivo;

string res;
int i = 20;
int fac;

fun int factorial(int n){
    int f;
    if(n <= 1){
        f = 1;
    }
    else {
        f = n * factorial( n - 1);
    }
    return f;
}

* {
    fac = factorial(i);
    res = "Factorial de " + i + " es: " + fac;

    print(res);
}